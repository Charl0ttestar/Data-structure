#include<stdio.h>  
  
int main(){  
  
 int n1,n2;  
 /* 輸入兩整數 */
 scanf("%d%d",&n1,&n2);
 
 /* 輸出其總和、乘積、差、商、和餘數 */
 printf("%d+%d=%d\n",n1,n2,n1+n2);  
 printf("%d*%d=%d\n",n1,n2,n1*n2);  
 printf("%d-%d=%d\n",n1,n2,n1-n2);  
 printf("%d/%d=%d...%d\n",n1,n2,n1/n2,n1%n2);  
  
  
  return 0;  
}  
