#include<stdio.h>  
#include<stdlib.h>  
  
int main(){  
  
char ID[11];

/* 檢查碼的對應表 */
char table[26][2]={{'1','0'},{'1','1'},{'1','2'},{'1','3'},  
                   {'1','4'},{'1','5'},{'1','6'},{'1','7'},  
                   {'3','4'},{'1','8'},{'1','9'},{'2','0'},  
                   {'2','1'},{'2','2'},{'3','5'},{'2','3'},  
                   {'2','4'},{'2','5'},{'2','6'},{'2','7'},  
                   {'2','8'},{'2','9'},{'3','2'},{'3','0'},  
                   {'3','1'},{'3','3'}};  
                   
int P,i,X1,X2,off;  

while(scanf("%s",ID)!=EOF){  
    off=0;
    
    /* ID[0]僅可能為A~Z */
    if('A'<=ID[0]<='Z'){
        /* ID[1]為性別欄,只有1或2 */
        if('1'<=ID[1]<='2'){
            /* 照著表找出字母對應的代號,從ascii碼轉成數字*/
            X1 = table[ID[0]-'A'][0] - '0';  
            X2 = table[ID[0]-'A'][1] - '0';  
            /* 照著公式算出檢查碼P */
            P = X1 + (9*X2);  
            for(i=8;i>0;i--)  
                P += i*(ID[9-i]-'0');  
            P += ID[9]-'0';  
            /* P無法被10整除,身分證號碼是錯的 */
            if(P%10!=0){  
                off=1;  
            }  
        }else{  
            off=1;  
        }  
    }else{  
      off=1;  
    }
    
    if(off==0){  
        printf("CORRECT!!!\n");  
    }else{  
        printf("WRONG!!!\n");  
    }  
}  
  
return 0;  
  
  
}  
